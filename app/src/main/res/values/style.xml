<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Shape.Circle" parent="ShapeAppearanceOverlay">
        <item name="cornerSize">50%</item>
    </style>

    <style name="TextAppearance.Title" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">@dimen/text_size_title</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>
    <style name="TextAppearance.Medium" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/roboto_medium</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="fontFamily">@font/roboto_medium</item>
    </style>

    <style name="TextAppearance.Small" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">@dimen/text_size_regular</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>
</resources>